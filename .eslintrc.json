{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "airbnb"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"no-use-before-define": 0,
		"indent": [true, "tabs", 2],
		"quotes": ["single", "double"],
		"react/prop-types": ["error", { "ignore": ["navigation"] }],
		"import/prefer-default-export": 0
	}
}
